  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug",
      "env":{
        "WORKSPACE_DIR": "${workspaceFolder}"
      },
      "cargo": {
        "args": [
          "build",
          "--bin=phoenix_wrust",
          "--package=phoenix_wrust"
        ],
        "filter": {
          "name": "phoenix_wrust",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Release",
      "env":{
        "WORKSPACE_DIR": "${workspaceFolder}"
      },
      "cargo": {
        "args": [
          "build",
          "--bin=phoenix_wrust",
          "--package=phoenix_wrust",
          "--release"
        ],
        "filter": {
          "name": "phoenix_wrust",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
